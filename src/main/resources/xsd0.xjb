<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
	version="2.1">

	<jaxb:globalBindings>
		<xjc:javaType name="java.time.LocalDate"
			xmlType="xs:date"
			adapter="com.migesok.jaxb.adapter.javatime.LocalDateXmlAdapter" />
		<xjc:javaType name="java.time.LocalDateTime"
			xmlType="xs:dateTime"
			adapter="com.migesok.jaxb.adapter.javatime.LocalDateTimeXmlAdapter" />
	</jaxb:globalBindings>

</jaxb:bindings>